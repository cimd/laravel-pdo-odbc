{
  "name": "konnec/laravel-snowflake",
  "description": "PDO Snowflake integration for Laravel framework",
  "type": "library",
  "homepage": "https://github.com/konnec/laravel-snowflake",
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "illuminate/database": ">=10.0",
    "illuminate/support": ">=10.0"
  },
  "autoload": {
    "psr-4": {
      "LaravelPdoOdbc\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelPdoOdbc\\ODBCServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}